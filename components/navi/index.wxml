<!--components/navi/index.wxml-->
<view class="container">
  <image
    class="icon"
    src="{{latest ? disLeftSrc : leftSrc}}"
    bind:tap="onLeftClick"
  />
  <text class="title">{{title}}</text>
  <image
    class="icon"
    src="{{first ? disRightSrc : rightSrc}}"
    bind:tap="onRightClick"
  />
</view>
